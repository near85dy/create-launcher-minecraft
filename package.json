  "repository": {
    "type": "git",
    "url": "git+https://github.com/near85dy/create-launcher-minecraft.git"
  },
  "license": "ISC",
  "author": "kirills",
  "type": "commonjs",
  "main": "dist/electron/main.js",
  "scripts": {
    "start": "concurrently -k \"npm run dev\" \"npm run electron\"",
    "dev": "vite dev",
    "build:react": "vite build",
    "build:electron": "electron-builder",
    "electron": "tsc && cd dist && electron electron/main.js"
  },
  "build": {
    "appId": "com.example.create_launcher",
    "productName": "Create launcher",
    "files": [
      "./dist/react/**",
      "./dist/electron/main.js"
    ],
    "directories": {
      "output": "release"
    },
    "win": {
      "sign": false,
      "target": "nsis",
      "certificateFile": "certs/mycert.pfx",
      "certificatePassword": "1234"
    }
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.7",
    "@types/node": "^24.2.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "typescript": "^5.9.2",
    "vite": "^5.0.0"
  },
  "dependencies": {
    "adm-zip": "^0.5.16",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.7.1"
  }
